{
  "name": "alecrabbit/php-console-spinner",
  "type": "library",
  "description": "Colorful spinner for php cli applications",
  "keywords": [
    "php",
    "spinner",
    "console",
    "cli",
    "scripts",
    "pretty",
    "clock",
    "arrow",
    "moon",
    "color",
    "colorful",
    "ansi",
    "snake"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/php-accessories": ">=0.6",
    "alecrabbit/php-console-colour": "^0.5 || ^0.6",
    "alecrabbit/php-helpers": "^0.5 || ^0.6"
  },
  "require-dev": {
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/var-dumper": "^4.2",
    "react/http": "^0.8",
    "fzaninotto/faker": "^1.8",
    "victorjonsson/markdowndocs": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\Spinner\\": "src\\Spinner"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\Spinner\\": "tests/"
    }
  },
  "minimum-stability": "alpha"
}

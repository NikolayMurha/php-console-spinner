{
  "name": "alecrabbit/php-console-spinner",
  "type": "library",
  "description": "Spinner - your task is running",
  "keywords": [
    "php",
    "spinner",
    "console",
    "scripts",
    "pretty",
    "clock",
    "moon",
    "snake"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/php-accessories": "^0.5",
    "alecrabbit/php-console-colour": "^0.2"
  },
  "require-dev": {
    "alecrabbit/php-helpers": "^0.5",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\Spinner\\": "src\\Spinner"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\Spinner\\": "tests/"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
